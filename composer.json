{
    "name": "sehrgut/eloquent-computed-attributes",
    "description": "Automatically compute attributes on Laravel Eloquent models when their input changes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Paul",
            "email": "joseph@sehrgute.software"
        }
    ],
    "require": {
        "illuminate/support": "^5.4|^5.5|^5.6|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "SehrGut\\EloquentComputedAttributes\\": "src/EloquentComputedAttributes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    }
}
